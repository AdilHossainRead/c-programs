#include<stdio.h>
#include<stdlib.h>
struct Listnode
{
    int data;
    struct Listnode* next;
};
int main()
{
    Listnode* node1=(Listnode*)malloc(sizeof(Listnode));
    node1->data=10;
    node1->next=NULL;

    Listnode* node2=(Listnode*)malloc(sizeof(Listnode));
    node2->data=20;
    node2->next=NULL;

    Listnode* node3=(Listnode*)malloc(sizeof(Listnode));
    node3->data=30;
    node3->next=NULL;


    Listnode* node4=(Listnode*)malloc(sizeof(Listnode));
    node4->data=40;
    node4->next=NULL;


    printf(" %d",node1->data);
    printf(" %d",node2->data);
    printf(" %d",node3->data);
    printf(" %d",node4->data);
}

