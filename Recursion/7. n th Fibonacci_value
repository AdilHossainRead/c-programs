#include<stdio.h>
int Fibonacci_th_value(int n)
{
    if(n==0)
    {
        return 0;
    }
    else if(n==1)
    {
        return 1;
    }
     return Fibonacci_th_value(n-1)+Fibonacci_th_value(n-2);
}
int main()
{
    int n;
    printf("Enter the number n=");
    scanf("%d",&n);
    int number=Fibonacci_th_value(n);
    printf("%d value is %d",n,number);

}

