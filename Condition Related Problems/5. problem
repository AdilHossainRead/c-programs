#include<stdio.h>
int main()
{
    int x,y,z;
    printf("enter X=" );
    scanf("%d",&x);
    printf("enter y=" );
    scanf("%d",&y);
    printf("enter z=" );
    scanf("%d",&x);
    if(x>y && x>z)
    {
        printf("max =%d",x);
    }
    if(y>x && y>z)
    {
        printf("max =%d",y);
    }
    if(z>x && z>y)
    {
        printf("max =%d",z);
    }
}
